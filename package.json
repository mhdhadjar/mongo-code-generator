{
  "name": "mongo-code-generator",
  "version": "0.1.0",
  "description": "Create MongoDb models from existed object in database.",
  "main": "./dist/main.js",
  "jest": {
    "testPathIgnorePatterns": [
      "./assets",
      "./dist",
      "./out",
      "./node_modules"
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
